<option prefix="Dis" parent="com.kopiright.util.base.Options"  package="com.kopiright.bytecode.dis"  usage="usage: com.kopiright.bytecode.dis.Main [option]* [--help] &lt;class-files, class-names>">
<param longname="stack" shortname="x"  type="boolean"  default="false"  help="Prints stack information" />
<param longname="sorted" shortname="s"  type="boolean"  default="false"  help="Prints fields and methods sorted" />
<param longname="stdout" shortname="c"  type="boolean"  default="false"  help="Prints on the standard output" />
<param longname="destination" shortname="d"  type="String"  default="null"  help="Sets the destination directory" />
<param longname="verbose" shortname="v"  type="boolean"  default="false"  help="Displays information while processing files" />
<param longname="inter" shortname="i"  type="boolean"  default="false"  help="Only displays the interfaces of classes" />
<param longname="extdirs" shortname="t"  type="String"  default="null"  help="Define location of installed extensions" />
<param longname="classpath" shortname="p"  type="String"  default="null"  help="Sets the classpath" />
</option>
