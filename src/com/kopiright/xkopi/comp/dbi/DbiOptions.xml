<?xml version="1.0" encoding="UTF-8"?>

<option prefix="Dbi" parent="com.kopiright.xkopi.comp.sqlc.SqlcOptions"  package="com.kopiright.xkopi.comp.dbi"  usage="usage: com.kopiright.xkopi.comp.dbi.Main [option]* [--help] &lt;sql-files>">
<param longname="simulate" shortname="t"  type="boolean"  default="false"  help="Runs the script but abort it" />
<param longname="stdout" shortname="s"  type="boolean"  default="false"  help="Prints on standard output" />
<param longname="stdin" shortname="e"  type="boolean"  default="false"  help="Reads from standard input" />
<param longname="syntax" shortname="S"  type="String"  default="jdbc"  help="Sets the syntax to use for output (jdbc | tbx | sap | pg | ora)" />
<param longname="beautify" shortname="b"  type="boolean"  default="false"  help="Prints the source code (beautifier)" />
<param longname="driver" shortname="D"  type="String"  default="null"  help="Sets the driver to use for DB access" />
<param longname="url" shortname="u"  type="String"  default="null"  help="Sets the url of the database" />
<param longname="login" shortname="l"  type="String"  default="null"  help="Sets the name of the user for login" />
<param longname="passwd" shortname="p"  type="String"  default="null"  help="Sets the passwd to login" />
<param longname="interfaceGen" shortname="i"  type="String"  default="null"  help="Sets the package name of Database.k" />
<param longname="iClassName" shortname="n"  type="String"  default="Database"  help="Set the interface class name (use with interfaceGen)" />
<param longname="sqlCase" shortname="C"  type="String"  default="null"  help="Change the case of SQL keywords. Only supported case: upper" />
<param longname="destination" shortname="d"  type="String"  default="null"  help="the location to generate the database interface" />
<param longname="commit" shortname="c"  type="String"  default="null"  optionalDefault="DICT"  help="add a transDB to the data dict." />
</option>
