<option prefix="Lpd" parent="com.kopiright.util.base.Options"  package="com.kopiright.util.lpr"  usage="usage: com.kopiright.lpd.Dummy">
<param longname="localHost" shortname="L"  type="String"  default="localhost"  help="Sets the local host" />
<param longname="printHost" shortname="H"  type="String"  default="localhost"  help="Sets the print hosts" />
<param longname="proxyHost" shortname="X"  type="String"  default="null"  help="Sets the proxy host to use (UNIX)" />
<param longname="remotePort" shortname="d"  type="int"  default="-1"  help="Sets the remote port" />
<param longname="sourcePort" shortname="s"  type="int"  default="-1"  help="Sets the source port" />
<param longname="timeout" shortname="t"  type="int"  default="60000"  help="Sets the timeout value in millisc" />
<param longname="bindSourcePort" shortname="B"  type="boolean"  default="false"  help="Binds the source port" />
<param longname="user" shortname="u"  type="String"  default="null"  help="Sets the user who invoked the command" />
<param longname="queue" shortname="P"  type="String"  default="null"  help="Sets the queue to work on" />
</option>
