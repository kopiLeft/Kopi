<?xml version="1.0" encoding="UTF-8"?>

<option prefix="LpR" parent="LpdOptions"  package="com.kopiright.util.lpr"  usage="usage: com.kopiright.LpR.Main [options] &lt;files>">
<param longname="copies" shortname="#"  type="int"  default="1"  help="Sets the number of copies to print" />
<param longname="filetype" shortname="f"  type="String"  default="l"  help="Sets the file type" />
<param longname="indent" shortname="i"  type="int"  default="-1"  optionalDefault="8"  help="Sets the indent size" />
<param longname="job" shortname="J"  type="String"  default="null"  help="Sets the job identifier" />
<param longname="mail" shortname="m"  type="boolean"  default="false"  help="Mails when job is done" />
<param longname="burst" shortname="b"  type="boolean"  default="false"  help="Prints a burst page" />
<param longname="remove" shortname="r"  type="boolean"  default="false"  help="Removes the file when printing is done" />
<param longname="dataFirst" shortname="D"  type="boolean"  default="false"  help="Sends data first (for old NT lpd servers)" />
<param longname="title" shortname="T"  type="String"  default="null"  help="Sets the title for the job" />
<param longname="tmpdir" shortname="t"  type="String"  default="/tmp/"  help="Sets the tmp dir to use" />
<param longname="width" shortname="w"  type="int"  default="-1"  optionalDefault="80"  help="Sets the page width" />
<param longname="windows" shortname="W"  type="boolean"  default="false"  help="Uses a windows style protocol to communicate with the server" />
<param longname="printClass" shortname="C"  type="String"  default="null"  help="Sets the print class" />
</option>
