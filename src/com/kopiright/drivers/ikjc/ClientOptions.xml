<?xml version="1.0" encoding="UTF-8"?>

<option prefix="Client" parent="com.kopiright.util.base.Options"  package="com.kopiright.drivers.ikjc"  usage="usage: com.kopiright.drivers.ikjc.Client [option]* &lt;files>">
